{
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=TargetBrowse;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Pinecone": {
    "ApiKey": "*"
  },
  "OpenAI": {
    "ApiKey": "OPENAI-API-KEY",
    "ChatModel": "gpt-4o-mini",
    "EmbeddingModel": "text-embedding-3-small" // You can change this to any OpenAI embedding model
  },
  "Azure": {
    "DocumentIntelligence": {
      "Endpoint": "*",
      "Key": "*"
    }
  },
  "YouTube": {
    "ApiKey": "YOUTTUBE-API-KEY",
    "MaxSearchResults": 50,
    "DailyQuotaLimit": 10000,
    "QuotaWarningThreshold": 80,
    "QuotaCriticalThreshold": 95,
    "MaxConcurrentRequests": 5,
    "RequestTimeoutSeconds": 30,
    "EnablePersistentQuotaStorage": true,
    "QuotaStorageFilePath": "",
    "QuotaResetHour": 0,
    "EnableQuotaLogging": true
  },
  "Apify": {
    "TokenId": "apify_token"
  },
  "ProjectSettings": {
    "MaxVideosPerProject": 10,
    "MinVideosForGuide": 3
  },
  "AllowedHosts": "*"
}